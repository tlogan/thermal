make_nat_stream : () => (
  ch : alloc_chan ();
  count : i => (
    sync (send (ch, i,));
    count (add (i, 1,))
  );
  spawn (() => count 33); 
  ch
);
nstream : make_nat_stream ();
loop : res => (
  case 0 => res 
  case n => (
    res' : sync (recv nstream);
    loop res' (sub (n, 1,))
  )
);

loop () 9